{
  "name": "aws-node-exp-ts",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "prebuild": "rimraf lib",
    "build": "tsc -p ./tsconfig.build.json",
    "build:esb": "rm -rf dist-esb && esbuild ./src/app/handler-vendia.ts --entry-names=[dir]/[name]/handler-vendia --bundle --minify --sourcemap --platform=node --target=node16.14 --outdir=dist-esb",
    "build:esb-c": "rimraf dist-esb && esbuild ./src/app/handler-vendia.ts --entry-names=[dir]/[name]/handler-vendia --bundle --sourcemap --platform=node --target=node16.14 --outdir=dist-esb",
    "lint": "eslint \"{src}/**/*.ts\" --fix",
    "local:dev-ts": "rimraf .build && export NODE_ENV=dev && sls offline --config sls-ts.yml",
    "local:dev-esb": "rimraf .esbuild && export NODE_ENV=dev && sls offline --config sls-esb.yml",
    "deploy": "serverless deploy",
    "test-jest-envs": "export JEST_JUNIT_OUTPUT_DIR=test-results/jest && export JEST_JUNIT_OUTPUT_FILE=test-results/jest/junit.xml && jest",
    "test:jest": "jest",
    "test:jest-c": "ts-node -O '{\"module\":\"commonjs\"}' node_modules/jest/bin/jest.js",
    "test:jest-ci": "jest --ci"
  },
  "dependencies": {
    "@vendia/serverless-express": "^4.9.0",
    "body-parser": "^1.20.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.17.1",
    "kcors": "^2.2.2",
    "lodash": "^4.17.21",
    "mongoose": "^6.4.4",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "routing-controllers": "^0.9.0",
    "serverless-http": "^2.7.0",
    "typedi": "^0.10.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.101",
    "@types/dotenv-safe": "^8.1.2",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.5",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^18.0.4",
    "@types/rewire": "^2.5.28",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "cross-env": "^7.0.3",
    "esbuild": "^0.14.49",
    "eslint": "^8.19.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.4",
    "eslint-plugin-promise": "^6.0.0",
    "jest": "^28.1.3",
    "jest-junit": "^14.0.0",
    "rewire": "^6.0.0",
    "serverless-esbuild": "^1.31.0",
    "serverless-offline": "^8.8.1",
    "serverless-plugin-typescript": "^2.1.2",
    "ts-jest": "^28.0.6",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}
